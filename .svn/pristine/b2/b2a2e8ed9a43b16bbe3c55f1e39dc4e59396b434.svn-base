      <Fields>
        <Field Name="dc_name">
          <DataField>dc_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>CRM</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select DF.dc_name from dc_foods DF
inner join dc_dc_foods_dc_meal_component MTM on MTM.dc_foodsid = DF.dc_foodsId
inner join dc_meal_component DMC on MTM.dc_meal_componentid = DMC.dc_meal_componentId
where DMC.dc_meal_componentId = '902024EA-6FFF-E011-9385-00155D0A0205'
and DF.dc_Common_Food = 1 and
DF.dc_CanUseFoodinMenu = 1
order by df.dc_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
